package com.amwell.util;

/**
 * 调用方法时返回的状态码
 * 
 * @author 番茄很忙
 * @version V1.0 @2017-7-24[版本号, YYYY-MM-DD]
 * @see [相关类/方法]
 * @since Application Service System.
 */
public class StatusCode {

	/**
	 * 验证成功
	 */
	public static final Integer SUCCESS = 1; // 返回成功

	/**
	 * 验证失败
	 */
	public static final Integer EXCEPTION = 0; // 异常
	
	/**
	 * 并发锁异常
	 */
	public static final Integer LOCKERROR = -1;//
	
	/**
	 * 登录超时
	 */
	public static final Integer LOGIN_OUT = -100;
	
	/**
	 * simcompany 对象为空
	 */
	public static final Integer SIMCOMPANY_NULL = -1001;
	
	/**
	 * 公司全称为空
	 */
	public static final Integer SIMCOMPANY_FULLNAME_NULL = -10011;
	
	/**
	 * 公司简称
	 */
	public static final Integer SIMCOMPANY_SHORTNAME_NULL = -10012;
	
	/**
	 * 管理员登录名
	 */
	public static final Integer SIMCOMPANY_LOGINNAME_NULL = -10013;
	
	/**
	 * simbaseInfo 对象为空
	 */
	public static final Integer SIMBASEINFO_NULL = -2001;
	
	/**
	 * MSISDN
	 */
	public static final Integer SIMBASEINFO_MSISDN_NULL = -20011;
	
	/**
	 * ICCID
	 */
	public static final Integer SIMBASEINFO_ICCID_NULL = -20012;
	
	/**
	 * 登录验证码不通过
	 */
	public static final Integer LOGIN_CODE = -3001;
}
